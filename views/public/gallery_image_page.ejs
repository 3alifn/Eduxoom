    <center>

        <h5 class="container text-center w-100 alert alert-secondary">Photos gallery </h5>

    </center>

 
    <div  class="row" id="listDataPrint">


   </div>


<%- include("../component/footer-nav.html") %> 


<script>


    
function loadContentData(){
        
        $.ajax({
            method: "post",
            url: "/pu/gallery/image/get",
            data: {},
            dataType: "JSON",
            success: function (info) {
    
                $("#listDataPrint").html(info.listData)
                
                setTimeout(() => {
                    $(".image-scope").removeClass("spinner-border")
                }, 3000);
            }
        });
    
    }

    loadContentData()
    

    
    
    
    
    
    
    
    
    </script>
