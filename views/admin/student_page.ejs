function todoStudent(){
  var formdata = new FormData($('form')[0]);
  $.ajax({
    method: "post",
    url: "/admin/student/join",
    data: formdata, 
    mimeTypes:"multipart/form-data",
    contentType: false,
    cache: false,
    processData: false,   
